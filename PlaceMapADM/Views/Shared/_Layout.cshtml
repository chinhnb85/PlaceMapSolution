    @Scripts.Render("~/bundles/datatable")

    @Scripts.Render("~/bundles/chosen")

    @Scripts.Render("~/bundles/editer")

    @Scripts.Render("~/bundles/base")

    @Scripts.Render("~/bundles/mustache")

    @Scripts.Render("~/bundles/noty")

<!--Page Related Scripts-->
